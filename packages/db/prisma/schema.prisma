// Prisma schema 파일 경로
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// 예시 모델
model User {
  id        Int      @id      @default(autoincrement())
  email     String   @unique
  nickname  String
  createdAt DateTime @default(now())
}
